{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "camelcase": ["off"],
        "kebab-case": ["off"],
        "react/jsx-filename-extension": [1, {"extensions" : [".js", ".jsx"]}],
        "react/prop-types": 0,
        "react/destructuring-assignment": [0], 
    }
}
